{
  "name": "@ds/vue",
  "version": "1.0.0",
  "description": "Vue wrappers for the design system web components",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ds/web-components": "workspace:*",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vue-tsc": "^2.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "keywords": [
    "vue",
    "design-system",
    "components"
  ]
}

